@model IEnumerable<SAGRE.Models.MetodosAnalise.ClassificaoOWAS>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NumCosta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NumBraco)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NumPernas)
