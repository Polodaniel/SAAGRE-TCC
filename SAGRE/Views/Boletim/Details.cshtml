@model SAGRE.Models.BoletimModel

@{
    ViewData["Title"] = "Detalhe do Boletim de Risco";
}

<div class="col mt-2">

    <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="#">Ínicio</a></li>
            <li class="breadcrumb-item active" aria-current="page">@ViewData["Title"]</li>
        </ol>
    </nav>

    <div class="row">

        <div class="col">
            <div class=" ml-1">
                <div class="card">
                    <div class="card-body">
                        <div class="row">
                            <div class="col">
                                <form asp-action="Create">
                                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                                    <p class="">
                                        <div class="alert alert-dark font-weight-bold" role="alert" data-toggle="" href="#MenuAnalisePostura" aria-expanded="false" aria-controls="MenuAnalisePostura">
                                            <i class="fas fa-paste mr-2"></i> Dados do Boletim
                                        </div>
                                    </p>

                                    <div class="row">
                                        <div class="col-2">
                                            @*<label asp-for="ID" class="control-label mb-0 small"></label>*@
                                            <label class="control-label mb-0 small">Código</label>
                                            <input asp-for="CodigoFormatado" class="form-control form-control-sm" id="CampoData" disabled />
                                            <span asp-validation-for="ID" class="text-danger"></span>
                                        </div>
                                        <div class="col">
                                            <div class="form-group">
                                                <label asp-for="Data" class="control-label mb-0 small"></label>
                                                <input type="date" asp-for="Data" class="form-control form-control-sm" id="CampoData" disabled />
                                                <span asp-validation-for="Data" class="text-danger"></span>
                                            </div>
                                        </div>
                                        <div class="col-6">
                                            <div class="form-group">
                                                <label class="control-label mb-0 small">Nome do Avaliador</label>
                                                <input asp-for="NomeFiscal" class="form-control form-control-sm" id="NomeAvaliador" disabled />
                                                <span asp-validation-for="NomeFiscal" class="text-danger"></span>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-6">
                                            <label asp-for="Setor" class="control-label mb-0 small"></label>
                                            <input asp-for="Setor" class="form-control form-control-sm" id="SetorSelecionado" disabled />
                                            <span asp-validation-for="Setor" class="text-danger"></span>
                                        </div>
                                        <div class="col">
                                            <label class="control-label mb-0 small">Hora Ínicio</label>
                                            <input type="time" asp-for="HoraInicio" class="form-control form-control-sm" id="" disabled />
                                            <span asp-validation-for="HoraInicio" class="text-danger"></span>
                                        </div>
                                        <div class="col">
                                            <div class="form-group">
                                                <label class="control-label mb-0 small">Hora Termino</label>
                                                <input type="time" asp-for="HoraTermino" class="form-control form-control-sm" id="" disabled />
                                                <span asp-validation-for="HoraTermino" class="text-danger"></span>
                                            </div>
                                        </div>
                                        <div class="col">
                                            <div class="form-group">
                                                <label class="control-label mb-0 small">Tempo Gasto</label>
                                                <input type="text" asp-for="TempoDuracao" class="form-control form-control-sm" id="NomeAvaliador" disabled />
                                                <span asp-validation-for="TempoDuracao" class="text-danger"></span>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col">
                                            <div class="row">
                                                <div class="col">
                                                    <div class="form-group">
                                                        <label asp-for="Local" class="control-label mb-0"></label>
                                                        <input asp-for="Local" class="form-control form-control-sm" id="LocalSelecionado" disabled>
                                                        <span asp-validation-for="Local" class="text-danger"></span>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col">
                                                    <div class="form-group">
                                                        <label class="control-label mb-0">Atividade</label>
                                                        <input asp-for="Atividade" class="form-control form-control-sm" id="AtividadeSelecionado" disabled>
                                                        <span asp-validation-for="Atividade" class="text-danger"></span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-6">
                                            <div class="form-group">
                                                <label asp-for="Descricao" class="control-label mb-0"></label>
                                                <textarea asp-for="Descricao" class="form-control form-control-sm" style="height:105px;" id="DescricaoSelecionado" disabled></textarea>
                                                <span asp-validation-for="Descricao" class="text-danger"></span>
                                            </div>
                                        </div>
                                    </div>

                                    <p>
                                        @{
                                            var QntOWAS = @Model.ListaAnalisePostura.Count;

                                            string AnaliseNasaTLX = String.Empty;
                                            string CorNasa = String.Empty;
                                            string CorLVE = String.Empty;

                                            if (@Model.listanasa.BoletimModel == null)
                                            {
                                                AnaliseNasaTLX = "Não";
                                                CorNasa = "text-danger";
                                            }
                                            else
                                            {
                                                AnaliseNasaTLX = "Sim";
                                                CorNasa = "text-success";
                                            }

                                            if (@ViewBag.AnaliseLVE == "Sim")
                                            {
                                                CorLVE = "text-success";
                                            }
                                            else
                                            {
                                                CorLVE = "text-danger";
                                            }

                                        }
                                        <div class="alert alert-dark small" role="alert">
                                            <div class="row font-weight-bold">
                                                <div class="col">
                                                    Análises Lançadas
                                                </div>
                                                <div class="col">
                                                    Análise Postura - OWAS: @QntOWAS
                                                </div>
                                                <div class="col">
                                                    Análise Cognitiva - NASA TLX: <span class="@CorNasa"> @AnaliseNasaTLX </span>
                                                </div>
                                                <div class="col">
                                                    Análise Ambiente - LVE: <span class="@CorLVE"> @ViewBag.AnaliseLVE </span>
                                                </div>
                                            </div>
                                        </div>
                                    </p>

                                    <p>
                                        <div class="alert alert-dark" role="alert" data-toggle="collapse" href="#MenuAnalisePostura" aria-expanded="false" aria-controls="MenuAnalisePostura">
                                            <i class="fas fa-street-view mr-2"></i> Análise Postura - OWAS
                                        </div>
                                    </p>
                                    <div class="collapse" id="MenuAnalisePostura">
                                        <div class="card card-body">

                                            <div class="row">

                                                @*@foreach (var item in ViewBag.AnalisePostura)*@

                                                @{
                                                    if (Model.ListaAnalisePostura.Count == 0)
                                                    {
                                                        <div class="col text-center">
                                                            <p class="mb-0 text-center" style="font-size:12px;"><span class="font-weight-bold font-italic"> Não foi adicionado nenhuma Análise de Postura !</span></p>
                                                        </div>
                                                    }
                                                }

                                                @foreach (var item in @Model.ListaAnalisePostura)
                                                {
                                                    var CorAcao = String.Empty;

                                                    if (@item.AcaoDescricao == "Não são necessarias medidas corretivas.")
                                                    {
                                                        CorAcao = "text-success";
                                                    }
                                                    else if (@item.AcaoDescricao == "São necessarias correções em um futuro próximo.")
                                                    {
                                                        CorAcao = "text-warning";
                                                    }
                                                    else if (@item.AcaoDescricao == "São necessárias correções tão logo quanto possível.")
                                                    {
                                                        CorAcao = "text-warning";
                                                    }
                                                    else if (@item.AcaoDescricao == "São necessárias correções imediatas.")
                                                    {
                                                        CorAcao = "text-danger";
                                                    }

                                                    <div class="col-6">
                                                        <div class="alert alert-secondary" role="alert">
                                                            <p class="mb-0" style="font-size:11.5px;"><span class="font-weight-bold"> Postura das Costa:</span> @item.ICDescricao</p>
                                                            <p class="mb-0" style="font-size:11.5px;"><span class="font-weight-bold"> Postura dos Braços:</span> @item.IBDescricao</p>
                                                            <p class="mb-0" style="font-size:11.5px;"><span class="font-weight-bold"> Postura das Pernas:</span> @item.IPDescricao</p>
                                                            <p class="mb-0" style="font-size:11.5px;"><span class="font-weight-bold"> Esforço:</span> @item.IEDescricao</p>
                                                            <p class="mb-0 @CorAcao" style="font-size:11.5px;"><span class="font-weight-bold"> Ação: @item.AcaoDescricao</span></p>
                                                        </div>
                                                    </div>
                                                }

                                            </div>

                                        </div>
                                    </div>

                                    <p>
                                        <div class="alert alert-dark" role="alert" data-toggle="collapse" href="#MenuAnaliseCorpo" aria-expanded="false" aria-controls="MenuAnaliseCorpo">
                                            <i class="fas fa-brain mr-2"></i> Análise Cognitiva - NASA TLX
                                        </div>
                                    </p>
                                    <div class="collapse" id="MenuAnaliseCorpo">
                                        <div class="card card-body">

                                            <div class="row">
                                                @{
                                                    var ValorTotal = ViewBag.ValorTotal;

                                                    if (Model.listanasa.BoletimModel != null)
                                                    {
                                                        <div class="col">
                                                            <table class='table table-sm small border-top-0'>
                                                                <thead>
                                                                    <tr>
                                                                        <th scope='col' class=''>FATOR</th>
                                                                        <th scope='col' class=''>PESO<p class='small mb-0'>(Comparação por pares)</p></th>
                                                                        <th scope='col' class=''>VALOR<p class='small mb-0'>(Comparação por pares)</p></th>
                                                                        <th scope='col' class=''>AJUSTE<p class='small mb-0'>(Peso X Valor)</p></th>
                                                                    </tr>
                                                                </thead>
                                                                <tbody>
                                                                    <tr>
                                                                        <th scope='row'>Demanda Mental</th>
                                                                        <td>  @Model.listanasa.escalaMental  </td>
                                                                        <td>  @Model.listanasa.rangeDM  </td>
                                                                        <td>  @ViewBag.ValorMental</td>
                                                                    </tr>
                                                                    <tr>
                                                                        <th scope='row'>Demanda Física</th>
                                                                        <td>  @Model.listanasa.escalaFisica  </td>
                                                                        <td>  @Model.listanasa.rangeDF  </td>
                                                                        <td>  @ViewBag.ValorFisico  </td>
                                                                    </tr>
                                                                    <tr>
                                                                        <th scope='row'>Demanda Temporal</th>
                                                                        <td>  @Model.listanasa.escalaTemporal  </td>
                                                                        <td>  @Model.listanasa.rangeDT  </td>
                                                                        <td>  @ViewBag.ValorTemporal  </td>
                                                                    </tr>
                                                                    <tr>
                                                                        <th scope='row'>Performace</th>
                                                                        <td>  @Model.listanasa.escalaPerformace  </td>
                                                                        <td>  @Model.listanasa.rangePE  </td>
                                                                        <td>  @ViewBag.ValorPerformace  </td>
                                                                    </tr>
                                                                    <tr>
                                                                        <th scope='row'>Esforço</th>
                                                                        <td>  @Model.listanasa.escalaEsforco  </td>
                                                                        <td>  @Model.listanasa.rangeDE  </td>
                                                                        <td>  @ViewBag.ValorEsforco  </td>
                                                                    </tr>
                                                                    <tr>
                                                                        <th scope='row'>Frustração</th>
                                                                        <td>  @Model.listanasa.escalaFrustacao  </td>
                                                                        <td>  @Model.listanasa.rangeFR  </td>
                                                                        <td>  @ViewBag.ValorFrustracao  </td>
                                                                    </tr>
                                                                    <tr>
                                                                        <th scope='row' colspan='2'>PONTUAÇÃO<p class='small mb-0'>(Soma dos Ajuste dividido por 15)</p></th>
                                                                        <td class='small'></td>
                                                                        @*<th>  @ViewBag.ValorTotal  </th>*@
                                                                        <th>  @ValorTotal  </th>
                                                                    </tr>
                                                                </tbody>
                                                            </table>
                                                        </div>
                                                    }
                                                    else
                                                    {
                                                        <div class="col text-center">
                                                            <p class="mb-0 text-center" style="font-size:12px;"><span class="font-weight-bold font-italic"> Não foi adicionado nenhuma Análise NASA-TLX !</span></p>
                                                        </div>
                                                    }
                                                }
                                            </div>

                                            <div class="row">
                                                <div class="col">
                                                    @{
                                                        if (Model.listanasa.BoletimModel != null)
                                                        {
                                                            if (ValorTotal >= 0 && ValorTotal <= 20)
                                                            {
                                                                <div class="alert alert-light" role="alert">
                                                                    Intensidade da Carga Mental: <strong>Mínimo</strong>
                                                                </div>
                                                            }
                                                            else if (ValorTotal >= 21 && ValorTotal <= 40)
                                                            {
                                                                <div class="alert alert-info" role="alert">
                                                                    Intensidade da Carga Mental: <strong>Baixa</strong>

                                                                </div>
                                                            }
                                                            else if (ValorTotal >= 41 && ValorTotal <= 60)
                                                            {
                                                                <div class="alert alert-success" role="alert">
                                                                    Intensidade da Carga Mental: <strong>Moderada</strong>
                                                                </div>
                                                            }
                                                            else if (ValorTotal >= 61 && ValorTotal <= 80)
                                                            {
                                                                <div class="alert alert-warning" role="alert">
                                                                    Intensidade da Carga Mental: <strong>Alta</strong>
                                                                </div>
                                                            }
                                                            else if (ValorTotal >= 81 && ValorTotal <= 100)
                                                            {
                                                                <div class="alert alert-danger" role="alert">
                                                                    Intensidade da Carga Mental: <strong>Intolerável</strong>
                                                                </div>
                                                            }
                                                        }
                                                    }
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <p>
                                        <div class="alert alert-dark" role="alert" data-toggle="collapse" href="#MenuAnaliseAmbiete" aria-expanded="false" aria-controls="MenuAnaliseAmbiete">
                                            <i class="fas fa-home mr-2"></i> Análise Ambiente - LVE
                                        </div>
                                    </p>
                                    <div class="collapse" id="MenuAnaliseAmbiete">
                                        <div class="card card-body">

                                            @{
                                                var CheckListBio = ViewBag.CheckListBio;
                                                var CheckListErg = ViewBag.CheckListErg;

                                                if (Equals(CheckListBio, null) && Equals(CheckListErg, null))
                                                {
                                                    <div class="col text-center mb-3">
                                                        <p class="mb-0 text-center" style="font-size:12px;"><span class="font-weight-bold font-italic"> Não foi adicionado nenhuma Análise Ambiente - LVE !</span></p>
                                                    </div>
                                                }
                                                else
                                                {
                                                    <p class="mb-0 mt-0 small">
                                                        <div class="row">
                                                            <div class="col col-sm col-md col-lg">
                                                                <div class="alert alert-dark small mb-0 pt-2 pb-2 font-weight-bold" style="font-size:11px;" role="alert" data-toggle="collapse" href="#CheckListUm" aria-expanded="false" aria-controls="MenuAnaliseAmbiete">
                                                                    CHECKLIST PARA AVALIAÇÃO DAS CONDIÇÕES BIOMECÂNICAS DO POSTO DE TRABALHO
                                                                </div>
                                                                <div class="collapse" id="CheckListUm">
                                                                    <div class="card card-body mt-1">
                                                                        @{
                                                                            if (!Equals(CheckListBio, null))
                                                                            {
                                                                                int Qnt = Convert.ToInt32(@ViewBag.ResultadoCheckListBio);

                                                                                if (Qnt == 14 || Qnt == 13)
                                                                                {
                                                                                    <div class="alert alert-success small" role="alert">
                                                                                        Condição Biomecânica Excelente !
                                                                                    </div>
                                                                                }
                                                                                else if (Qnt >= 10 && Qnt <= 12)
                                                                                {
                                                                                    <div class="alert alert-primary small p-2" role="alert">
                                                                                        Boa Condição Biomecânica !
                                                                                    </div>
                                                                                }
                                                                                else if (Qnt >= 7 && Qnt <= 9)
                                                                                {
                                                                                    <div class="alert alert-secondary small" role="alert">
                                                                                        Condição Biomecânica Razoável !
                                                                                    </div>
                                                                                }
                                                                                else if (Qnt >= 4 && Qnt <= 6)
                                                                                {
                                                                                    <div class="alert alert-warning small" role="alert">
                                                                                        Condição Biomecânica Ruim !
                                                                                    </div>
                                                                                }
                                                                                else if (Qnt <= 3)
                                                                                {
                                                                                    <div class="alert alert-danger small" role="alert">
                                                                                        Condição Biomecânica Péssima !
                                                                                    </div>
                                                                                }

                                                                                <div class="row">
                                                                                    <div class="col-3 col-sm-3 col-md-3 col-lg-3">
                                                                                        <div class=" p-1 nav flex-column nav-pills" id="v-pills-tab" role="tablist" aria-orientation="vertical">
                                                                                            <a class="card mb-1 nav-link active btn btn-sm" id="v-pills-home-tab" data-toggle="pill" href="#v-pills-bancada" role="tab" aria-controls="v-pills-bancada" aria-selected="true">Bancada</a>
                                                                                            <a class="card mb-1 nav-link btn btn-sm" id="v-pills-profile-tab" data-toggle="pill" href="#v-pills-acoes" role="tab" aria-controls="v-pills-acoes" aria-selected="false">Ações</a>
                                                                                            <a class="card mb-1 nav-link btn btn-sm" id="v-pills-messages-tab" data-toggle="pill" href="#v-pills-sentado" role="tab" aria-controls="v-pills-sentado" aria-selected="false">Sentado</a>
                                                                                        </div>
                                                                                    </div>
                                                                                    <div class="col col-sm col-md col-lg">
                                                                                        <div class="tab-content" id="v-pills-tabContent">
                                                                                            <div class="tab-pane fade show active" id="v-pills-bancada" role="tabpanel" aria-labelledby="v-pills-home-tab">
                                                                                                <table class="table table-sm table-bordered">
                                                                                                    <thead class="thead-dark">
                                                                                                        <tr>
                                                                                                            <th class="small text-center" style="width: 80%;">Pergunta</th>
                                                                                                            <th class="small text-center">Resposta</th>
                                                                                                        </tr>
                                                                                                    </thead>
                                                                                                    <tbody>
                                                                                                        <tr>
                                                                                                            <td class="small">
                                                                                                                A bancada de trabalho/máquIna está localizada em altura correta
                                                                                                                (trabalho pesado: ao nível do púbis / trabalho moderado: na altura
                                                                                                                do cotovelo / trabalho teve: a 30 cm dos olhos)?
                                                                                                            </td>
                                                                                                            <td class="align-middle" id="grupo1pergunta1">
                                                                                                                <select class="custom-select custom-select-sm" disabled>
                                                                                                                    @{ if (CheckListBio.Questao01 == "1")
                                                                                                                        {
                                                                                                                            <option> Sim </option> }
                                                                                                                        else
                                                                                                                        {
                                                                                                                            <option> Não </option> }}
                                                                                                                </select>
                                                                                                            </td>
                                                                                                        </tr>
                                                                                                        <tr>
                                                                                                            <td class="small">
                                                                                                                A bancada ou máquina tem regulagem de altura deforma a possibilitar
                                                                                                                ao trabalhador adequar a altura do posto de trabalho á sua?
                                                                                                            </td>
                                                                                                            <td class="align-middle" id="grupo1pergunta2">
                                                                                                                <select class="custom-select custom-select-sm" disabled>
                                                                                                                    @{ if (CheckListBio.Questao02 == "1")
                                                                                                                        {
                                                                                                                            <option> Sim </option> }
                                                                                                                        else
                                                                                                                        {
                                                                                                                            <option> Não </option> }}
                                                                                                                </select>
                                                                                                            </td>
                                                                                                        </tr>
                                                                                                        <tr>
                                                                                                            <td class="small">
                                                                                                                Os objetos e materiais de uso freqüente estão dentro da área de alcance?
                                                                                                            </td>
                                                                                                            <td class="align-middle" id="grupo1pergunta3">
                                                                                                                <select class="custom-select custom-select-sm" disabled>
                                                                                                                    @{ if (CheckListBio.Questao03 == "1")
                                                                                                                        {
                                                                                                                            <option> Sim </option> }
                                                                                                                        else
                                                                                                                        {
                                                                                                                            <option> Não </option> }}
                                                                                                                </select>
                                                                                                            </td>
                                                                                                        </tr>
                                                                                                    </tbody>
                                                                                                </table>
                                                                                            </div>
                                                                                            <div class="tab-pane fade" id="v-pills-acoes" role="tabpanel" aria-labelledby="v-pills-profile-tab">
                                                                                                <table class="table table-sm table-bordered">
                                                                                                    <thead class="thead-dark">
                                                                                                        <tr>
                                                                                                            <th class="small text-center" style="width: 80%;">Pergunta</th>
                                                                                                            <th class="small text-center">Resposta</th>
                                                                                                        </tr>
                                                                                                    </thead>
                                                                                                    <tbody>
                                                                                                        <tr>
                                                                                                            <td class="small">
                                                                                                                Tem-se que sustentar pesos com os membros superiores para evitar seu
                                                                                                                deslocamento seja na vertical seja na horizontal?
                                                                                                            </td>
                                                                                                            <td class="align-middle" id="grupo1pergunta4">
                                                                                                                <select class="custom-select custom-select-sm" disabled>
                                                                                                                    @{ if (CheckListBio.Questao04 == "1")
                                                                                                                        {
                                                                                                                            <option> Não </option> }
                                                                                                                        else
                                                                                                                        {
                                                                                                                            <option> Sim </option> }}
                                                                                                                </select>
                                                                                                            </td>
                                                                                                        </tr>
                                                                                                        <tr>
                                                                                                            <td class="small">
                                                                                                                Tem-se que apertar pedais estando de pé, em freqüência maior que 3 vezes por minuto?
                                                                                                            </td>
                                                                                                            <td class="align-middle" id="grupo1pergunta5">
                                                                                                                <select class="custom-select custom-select-sm" disabled>
                                                                                                                    @{ if (CheckListBio.Questao05 == "1")
                                                                                                                        {
                                                                                                                            <option> Não </option> }
                                                                                                                        else
                                                                                                                        {
                                                                                                                            <option> Sim </option> }}
                                                                                                                </select>
                                                                                                            </td>
                                                                                                        </tr>
                                                                                                        <tr>
                                                                                                            <td class="small">
                                                                                                                O trabalho exige a elevação dos braços acima do nível dos ombros?
                                                                                                            </td>
                                                                                                            <td class="align-middle" id="grupo1pergunta6">
                                                                                                                <select class="custom-select custom-select-sm" disabled>
                                                                                                                    @{ if (CheckListBio.Questao06 == "1")
                                                                                                                        {
                                                                                                                            <option> Não </option> }
                                                                                                                        else
                                                                                                                        {
                                                                                                                            <option> Sim </option> }}
                                                                                                                </select>
                                                                                                            </td>
                                                                                                        </tr>
                                                                                                        <tr>
                                                                                                            <td class="small">O trabalho exige ficar parado na posição de pé durante grande parte do tempo (mais que 60%)?</td>
                                                                                                            <td class="align-middle" id="grupo1pergunta7">
                                                                                                                <select class="custom-select custom-select-sm" disabled>
                                                                                                                    @{ if (CheckListBio.Questao07 == "1")
                                                                                                                        {
                                                                                                                            <option> Não </option> }
                                                                                                                        else
                                                                                                                        {
                                                                                                                            <option> Sim </option> }}
                                                                                                                </select>
                                                                                                            </td>
                                                                                                        </tr>
                                                                                                        <tr>
                                                                                                            <td class="small"> Os membros superiores têm que sustentar pesos? </td>
                                                                                                            <td class="align-middle" id="grupo1pergunta8">
                                                                                                                <select class="custom-select custom-select-sm" disabled>
                                                                                                                    @{ if (CheckListBio.Questao08 == "1")
                                                                                                                        {
